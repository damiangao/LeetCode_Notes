# not elegant solution
class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        op = ['(', '[', '{']
        ed = [')', ']', '}']
        s = list(s)
        stack = []
        for x in s:
            if x in ed:
                if not stack:
                    return False
                else:
                    if x != ed[op.index(stack[-1])]:
                        return False
                    else:
                        stack.pop()
            else:
                stack.append(x)
        
        if stack:
            return False
        return True
