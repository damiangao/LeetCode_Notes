class Solution(object):
    def hasCycle(self, head):
        """
        :type head: ListNode
        :rtype: bool
        """
        p1, p2 = head, head
        while p1:
            p1 = p1.next
            if p1:
                p1 = p1.next
                p2 = p2.next
                if p1 == p2:
                    return True
        return False
